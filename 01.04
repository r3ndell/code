#***********Двухфакторный ДА*****
install.packages("HSAUR2")
library(HSAUR2)
data(weightgain)
weightgain
boxplot(weightgain$weightgain ~ weightgain$type)
boxplot(weightgain$weightgain ~ weightgain$source)
F_2<-aov(weightgain$weightgain~weightgain$source+weightgain$type)
summary(F_2)
